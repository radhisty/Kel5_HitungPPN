public class aldiansyah293IVG {  
    public static void main(String[] args) {
        //deklarasi variabel
        String nama;
        int nim;

        //pengisian variabel
        nama = "Ary Prakosa";
        nim = 294;

        //menampilkan
        System.out.println("NAMA\t  : " + nama);
        System.out.println("NIM\t  : " + nim);
         
    }

}
